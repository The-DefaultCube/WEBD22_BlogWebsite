<%-include("partials/header")-%>
<h1>Compose</h1>
<form action="/compose" method="post">
	<div class="form-group">
		<label for="postTitle">Title</label>
		<input
			class="form-control"
			name="postTitle"
			placeholder="Post Title"
			type="text"
			id="postTitle"
			autocomplete="off"
			required
			autofocus
		/>
	</div>
	<div class="form-group">
		<label for="postContent">Post</label>
		<textarea
			class="form-control"
			id="postContent"
			rows="8"
			name="postContent"
			placeholder="Write Some Post Here ..."
			required
		></textarea>
	</div>
	<button class="btn btn-primary" type="submit" name="publish">
		Publish
	</button>
</form>
<%-include("partials/footer")-%>
